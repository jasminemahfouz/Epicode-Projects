@model Verbale

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Verbale</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Numero Verbale
        </dt>
        <dd class="col-sm-10">
            @Model.NumeroVerbale
        </dd>
        <dt class="col-sm-2">
            Data Verbale
        </dt>
        <dd class="col-sm-10">
            @Model.DataVerbale
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-secondary" asp-action="Index" asp-controller="Violazioni" asp-route-trasgressoreId="@ViewBag.TrasgressoreId">Back to Violazioni</a>
</div>
